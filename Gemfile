  gem 'rake'
	gem 'jekyll'
	gem 'jekyll-assets'
  gem 'jekyll-paginate'
  gem 'dotenv'
  gem 'stringex'
  gem 'pygments.rb'
	gem 'yui-compressor'
	gem 'closure-compiler'
  gem 'aws-sdk-v1'
  gem 'highline'
  gem 'byebug'
end

